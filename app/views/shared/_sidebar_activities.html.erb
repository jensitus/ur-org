<% sidebar_activities.each do |side_activity| %>
    <small>
      <%#= link_to side_activity.owner.name, side_activity.owner if side_activity.owner %>
    </small>
    <% if side_activity.trackable_type == 'Relationship' %>
        <%#= render_activities @sidebar_activities %>
        <% if side_activity.key == 'relationship.create' %>
            <%= render_activity side_activity %>
            <br>
        <% end %>
    <% elsif side_activity.trackable_type == 'Comment' %>
        <%#= render_activity side_activity %>
    <% elsif side_activity.trackable_type == 'PhotoComment' %>
        <%= render_activity side_activity %>
    <% end %>
    <small>

      <%#= side_activity.inspect %>


    </small>

<% end %>