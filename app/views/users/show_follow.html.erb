<% provide(:title, @title) %>
<div class="row">
  <aside class="col-md-4">
    <% if @user == current_user %>
        <%= render 'shared/sidebar' %>
    <% else %>
        <%= render 'shared/user_show_sidebar' %>
    <% end %>
  </aside>
  <div class="col-md-8">
    <h4><%= @title %></h4>
    <% if @users.any? %>
        <ul class="users follow">
          <%= render @users %>
        </ul>
        <%= paginate @users %>
    <% end %>
  </div>
</div>